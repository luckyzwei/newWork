/* 体现页面样式 */

page {
  background: #f8f8f8;
}

.getCash {
  width: 100%;
}

/* 可提现金额 */

.getCash .cash-number {
  width: 96%;
  margin: 0 auto;
}
.getCash .cash-number .money{
font-size: 40rpx;
padding: 8rpx 0;
}
/* icon */

.getCash .cash-number .cash-icon {
  width: 300rpx;
  height: 143rpx;
  margin-bottom: 10rpx;
}

/* 提现按钮 */

.getCash .cash-number .btn {
  width:100%;
  height: 75rpx;
  line-height: 75rpx;
  color: #fff;
  text-align: center;
  border-radius: 2px;
  margin: 45rpx 0 35rpx 0;
}
/* 提现记录 /明细 */
.getCash  .cash-lists{
  width: 100%;
  background: #fff;
}
.getCash  .cash-lists .list-items{
  width: 96%;
  margin: 0 auto;
  border-bottom: 1px solid #f8f8f8;
  padding: 20rpx 0;
}
/* 右部箭头 */
.getCash  .cash-lists .list-items .nav-right{
  width: 20rpx;
  height: 18rpx;
}