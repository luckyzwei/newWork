/* 体现页面样式 */

page {
  background: #f8f8f8;
}

.getCash {
  width: 100%;
}

/* 可提现金额 */

.getCash .cash-number {
  width: 96%;
  margin: 0 auto;
}
.getCash .cash-number .money{
font-size: 40rpx;
padding: 8rpx 0;
}
/* icon */

.getCash .cash-number .cash-icon {
  width: 300rpx;
  height: 143rpx;
  margin-bottom: 10rpx;
}

/* 提现按钮 */

.getCash .cash-number .btn {
  width:100%;
  height: 75rpx;
  line-height: 75rpx;
  color: #fff;
  text-align: center;
  border-radius: 2px;
  margin: 45rpx 0 35rpx 0;
}
/* 提现记录 /明细 */
.getCash  .cash-lists{
  width: 100%;
  background: #fff;
}
.getCash  .cash-lists .list-items{
  width: 96%;
  margin: 0 auto;
  border-bottom: 1px solid #f8f8f8;
  padding: 20rpx 0;
}
/* 右部箭头 */
.getCash  .cash-lists .list-items .nav-right{
  width: 20rpx;
  height: 18rpx;
}

/* 提现弹窗样式 */
/* 提现弹出层 */
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  bottom: 0;
  background: #000;
  left: 0;
  opacity: .2;
  z-index: 900;
}

.dialog-content {
    position: relative;
    width: 100%;
    padding-bottom: 25px;
    color: #666666;
    font-size: 12px;
    font-weight: bold;
    overflow: hidden;
    line-height: 14px;
    background-color: #FFFFFF;
}
.dialog-content .item-input .withdraw-money-header{
    position: relative;
    display: block;
    padding: 0 30rpx;
}

.dialog-content .item-input .withdraw-money-header .input-tips {
    position: relative;
    display: inline-block;
    text-align: left;
    width: 40%;
    margin-top: 10px;
    margin-bottom: 10px;
    color: #A2A6Ad;
    font-size: 12px;
    font-weight: normal;
}

.dialog-content .item-input .withdraw-money-header .withdraw-all {
    position: relative;
    display: inline-block;
    text-align: right;
    width: 40%;
    margin-top: 10px;
    margin-bottom: 10px;
    color: #5099D2;
    font-size: 12px;
    font-weight: normal;
}

.dialog-content .item-input .withdraw-title {
    position: relative;
    text-align: left;
    margin-left: 16px;
    margin-right: 16px;
    margin-top: 10px;
    margin-bottom: 10px;
    color: #A2A6Ad;
    font-size: 14px;
    font-weight: normal;
}

.dialog-content .item-input .withdraw-group {
    display: block;
    height: 64rpx;
    margin-left: 32rpx;
    box-sizing: border-box;
    background-color: #FFFFFF;
    border: none;
    text-align: left;
    color: #404245;
    font-size: 45rpx;
    font-weight: normal;
    line-height: 45rpx;
}


.dialog-content .item-input .withdraw-group .control-label {
    display: inline-block;
    width: 7%;
    box-sizing: border-box;
    background-color: transparent;
    color: #404245;
    font-size: 36rpx;
    font-weight: normal;
}

.dialog-content .item-input .withdraw-group .input_money {
    display: inline-block;
    height: 46rpx;
    width: 80%;
    margin-right: 16rpx;
    box-sizing: border-box;
    background-color: transparent;
    border: none;
    text-align: left;
    color: #404245;
    font-size: 45rpx;
    font-weight: normal;
    line-height: 45rpx;
}



.dialog-content .item-input .horizontal-line {
    height: 1rpx;
    margin-left: 15px;
    margin-right: 15px;
    display: block;
    background-color: #c7c7c7;
    /* vertical-align: middle; */
    font-size: 0;
}

.dialog-content .item-input .input_memo {
    display: block;
    width: 90%;
    height: 64px;
    margin-left: 16px;
    margin-right: 16px;
    box-sizing: border-box;
    text-align: left;
    overflow: hidden;
    color: #404245;
    font-size: 14px;
    font-weight: normal;
    background: #FBFBFB;
    border: 1px solid #E8EAED;
}

.dialog-button {
    position: relative;
    width: 100%;
    height: 44px;
    overflow: hidden;
    text-decoration: none;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.dialog-button .button {
    float: left;
    position: relative;
    display: block;
    width: 50%;
    height: 44px;
    box-sizing: border-box;

    font-size: 14px;
    font-weight: normal;
    text-align: center;
    line-height: 44px;
}



.dialog-button .button.left {
    font-weight: normal;
    color: #404245;
}

.dialog-button .button.right {
    font-weight: bold;
    background-color: #404245;
    color: #FFFFFF;
}